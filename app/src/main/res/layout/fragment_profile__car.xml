<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"

        tools:context=".Profile_CarFragment">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical"
            android:background="#FFFFFF">

            <include
                android:id="@+id/profilecustomtoolbar"
                layout="@layout/profilecustombar" />

            <RelativeLayout
                android:paddingTop="10dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center">


                <ImageView
                    android:id="@+id/imageView"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@drawable/imglogo" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/homescreenbg"
                    android:drawableRight="@drawable/camera"
                    android:layout_marginLeft="80dp" />

            </RelativeLayout>

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="#33333380"
                android:textSize="14sp"
                android:layout_marginBottom="10dp"
                android:text="Company Name" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:textSize="16sp"
                android:textColor="#040D1B"
                android:background="@drawable/edittextbg" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="16dp"
                android:textSize="16sp"
                android:layout_marginBottom="8dp"
                android:textColor="#33333380"
                android:text="Car Service location" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:text="Ar Rabwah, Riyadh, Saudi Arabia"
                android:padding="10dp"
                android:textSize="16sp"
                android:textColor="#040D1B"
                android:background="@drawable/edittextbg" />

            <ImageView
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:src="@drawable/map1_2" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:textColor="#33333380"
                android:textSize="14sp"
                android:layout_marginBottom="10dp"
                android:text="Phone" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="@drawable/edittextbg"
                android:textSize="16sp" />

            <TextView
                android:layout_marginTop="5dp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Email (optional)"
                android:textColor="#33333380"
                android:textSize="14sp"
                android:layout_marginBottom="5dp" />

            <EditText
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:padding="10dp"
                android:background="@drawable/edittextbg"
                android:textSize="16sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:text="Branch Description"
                android:textColor="#33333380"
                android:textSize="14sp"
                android:layout_marginTop="5dp"
                android:layout_marginBottom="5dp" />

            <androidx.appcompat.widget.AppCompatEditText
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:padding="10dp"
                android:textAlignment="textStart"
                android:background="@drawable/edittextbg"
                android:textSize="16sp" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="10dp"
                android:text="Working Hours"
                android:textColor="#333333"
                android:textSize="18dp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="horizontal"
                android:background="#FFFFFF">

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="vertical">

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />

                    <TextView
                        android:textAlignment="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Fri"
                        android:layout_margin="2dp"
                        android:padding="3dp"
                        android:background="#FFFFFF"
                        android:textSize="16sp"
                        android:textColor="#333333" />
                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="2dp"
                    android:orientation="vertical">

                    <Switch
                        android:layout_margin="2dp"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />

                    <Switch
                        android:layout_margin="2dp"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />

                    <Switch
                        android:layout_margin="2dp"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />

                    <Switch
                        android:layout_weight="1"
                        android:layout_margin="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />

                    <Switch
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />

                    <Switch
                        android:layout_weight="1"
                        android:layout_margin="2dp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />

                    <Switch
                        android:layout_margin="2dp"
                        android:layout_weight="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/switchbg" />
                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="#E4E4E4"
                    android:layout_margin="2dp"
                    android:padding="2dp"
                    android:orientation="vertical">

                    <EditText
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_margin="2dp"
                        android:background="@drawable/workingedittext"
                        android:hint="Start Time"
                        android:padding="2dp" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Off"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                </LinearLayout>

                <LinearLayout
                    android:layout_weight="1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:layout_marginTop="2dp"
                    android:background="#E4E4E4"
                    android:layout_margin="2dp"
                    android:padding="2dp"
                    android:orientation="vertical">

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                    <EditText
                        android:layout_margin="2dp"
                        android:padding="2dp"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="Start Time"
                        android:background="@drawable/workingedittext" />

                </LinearLayout>
            </LinearLayout>


            <Button
                android:layout_marginTop="20dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/update"
                android:layout_margin="10dp"
                android:padding="10dp"
                android:textColor="#FFFFFF"
                android:background="@drawable/btnbg"
                android:textAllCaps="false"
                android:layout_gravity="center" />

        </LinearLayout>

    </androidx.core.widget.NestedScrollView>
</layout>